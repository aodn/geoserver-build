<?xml version="1.0" encoding="utf-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

    <modelVersion>4.0.0</modelVersion>
    <groupId>au.org.emii</groupId>
    <artifactId>geoserver</artifactId>
    <packaging>pom</packaging>
    <version>1.0.0</version>

    <name>AODN Geoserver (pom)</name>

    <modules>
        <module>geotools</module> 
        <!-- xslt included via profile --> 
        <module>geoserver/src/wfs</module> <!-- needed for csv output dep resolution --> 
        <module>geoserver/src/extension/wps/wps-core</module> <!-- activation by profile wont add gs-wps-core to reactor deps -->
        <module>geoserver/src/extension/wps/web-wps</module>
        <module>geoserver/src/web</module>
        <module>geoserver-layer-filter-extension</module>
        <module>ncdfgenerator</module>
        <module>emii</module>
    </modules>

    <description>AODN Geoserver v${project.version}</description>
    <url>http://www.emii.org.au</url>

    <properties>
        <geoserver.version>2.7.0</geoserver.version>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.resources.sourceEncoding>${project.build.sourceEncoding}</project.resources.sourceEncoding>
    </properties>

</project>
